# This environment variable gives us a way to move the install
# location.
if [ -z "$USE_GEOCAL_SKELETONTOP" ]; then
    export USE_GEOCAL_SKELETONTOP=@prefix@
fi

source @geocaldir@/setup_geocal.sh

if [ -n "$PYTHONPATH" ]; then
  export PYTHONPATH=${USE_GEOCAL_SKELETONTOP}/@pythondir@:${USE_GEOCAL_SKELETONTOP}/@platpythondir@:${PYTHONPATH}
else
  export PYTHONPATH=${USE_GEOCAL_SKELETONTOP}/@pythondir@:${USE_GEOCAL_SKELETONTOP}/@platpythondir@
fi
if [ -n "$PATH" ]; then
  export PATH=${USE_GEOCAL_SKELETONTOP}/bin:@pythonpath@:${PATH}
else
  export PATH=${USE_GEOCAL_SKELETONTOP}/bin:@pythonpath@
fi
