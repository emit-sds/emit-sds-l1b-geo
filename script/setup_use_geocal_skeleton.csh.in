# This environment variable gives us a way to move the install
# location.
if(! $?USE_GEOCAL_SKELETONTOP) then
    setenv USE_GEOCAL_SKELETONTOP @prefix@
endif

source @geocaldir@/setup_geocal.csh

if($?PYTHONPATH) then
  setenv PYTHONPATH ${USE_GEOCAL_SKELETONTOP}/@pythondir@:${USE_GEOCAL_SKELETONTOP}/@platpythondir@:${PYTHONPATH}
else
  setenv PYTHONPATH ${USE_GEOCAL_SKELETONTOP}/@pythondir@:${USE_GEOCAL_SKELETONTOP}/@platpythondir@
endif
if ($?PATH) then
  setenv PATH ${USE_GEOCAL_SKELETONTOP}/bin:@pythonpath@:${PATH}
else
  setenv PATH ${USE_GEOCAL_SKELETONTOP}/bin:@pythonpath@
endif

